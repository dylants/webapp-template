{
  "name": "webapp-template",
  "version": "0.1.0",
  "engines": {
    "node": "10.15.3",
    "yarn": "1.13.0"
  },
  "private": true,
  "scripts": {
    "build:client": "NODE_ENV=production webpack --config config/webpack.config.prod.js",
    "build": "yarn clean:build && yarn build:client",
    "clean:build": "npx rimraf build",
    "clean:coverage": "npx rimraf coverage",
    "clean:node_modules": "npx rimraf **/node_modules",
    "clean": "yarn clean:build && yarn clean:coverage && yarn clean:node_modules",
    "lint": "eslint .",
    "production": "NODE_ENV=production node ./src/server/index.js",
    "start": "NODE_ENV=development nodemon src/server/index.js --config 'config/nodemon.json'",
    "test:jest": "NODE_ENV=test jest --config config/jest/index.js",
    "test:watch": "yarn test:jest --watch",
    "test": "yarn lint && yarn test:jest"
  },
  "dependencies": {
    "@emotion/core": "10.0.7",
    "@emotion/styled": "10.0.7",
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "glob": "7.1.3",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-hot-loader": "4.8.0",
    "react-redux": "6.0.1",
    "react-router-dom": "4.4.0-beta.7",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.8",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "css-loader": "2.1.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.10.0",
    "eslint": "5.15.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "fetch-mock": "7.3.1",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "progress-bar-webpack-plugin": "1.12.1",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "supertest": "4.0.0",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-dev-middleware": "3.6.1",
    "webpack-hot-middleware": "2.24.3"
  }
}

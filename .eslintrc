env:
  jest: true
extends:
  - airbnb
  - prettier
parser: babel-eslint
plugins:
  - prettier
root: true
rules:
  import/no-extraneous-dependencies: 0
  prettier/prettier: 2
  react/jsx-filename-extension: 0
  react/jsx-one-expression-per-line: 0
  sort-keys:
    - error
    - asc
    - caseSensitive: true
      natural: false
overrides:
  - files:
    - "*.test.js"
    rules:
      global-require: 0
